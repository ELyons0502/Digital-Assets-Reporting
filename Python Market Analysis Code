dfx = dfbnch[['Close']]
dfx = dfx.reset_index()
dfx = dfx.pivot(index='Date', columns='Ticker', values='Close')
dfx.rename({'^FTSE':'FTSE 100', '^GSPC':'S&P 500', '^IXIC':'Nasdaq'}, axis=1, inplace=True)
